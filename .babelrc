{
  "presets": [
    ["env", { "modules": false }],
    "react",
    "stage-0",
    "stage-1"
  ],
  "plugins": [
    ["import",{
      "libraryName": "antd",
      "libraryDirectory": "es", "style": true
    }],
    ["prismjs", {
      "languages": ["ts","less", "tsx","json"],
      "plugins": ["line-numbers"],
      "theme": "twilight",
      "css": true
    }],
    "transform-runtime",
    "syntax-dynamic-import",
    "react-hot-loader/babel"
  ]
}